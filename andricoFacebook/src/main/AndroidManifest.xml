<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.andrico.andrico">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Synchronize"
            android:label="@string/synchronize"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/settings"></activity>
        <activity
            android:name=".ContactList"
            android:label="@string/contacts"></activity>
        <activity
            android:name=".ContactInfo"
            android:label="@string/contact_info"></activity>
        <activity
            android:name=".WebActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/loading_page"></activity>
        <activity
            android:name=".facebook.LoginActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/login_label"></activity>
    </application>
    <uses-library android:name="com.google.android.maps" />
</manifest>